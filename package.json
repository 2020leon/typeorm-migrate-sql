{
  "name": "typeorm-migrate-sql",
  "version": "0.0.1",
  "description": "Generate Sql Files and Migrate",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "/lib",
    "/types"
  ],
  "bin": {
    "typeorm-migrate-sql": "./lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "rm -rf lib types && tsc",
    "version": "scripts/version.sh",
    "format": "prettier --write \"src/**/*.ts\" \"./**/*.json\"",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^10.0.0"
  },
  "peerDependencies": {
    "mysql2": "^2.2.5 || ^3.0.1",
    "ts-node": "^10.7.0",
    "typeorm": "^0.3.12"
  },
  "peerDependenciesMeta": {
    "ts-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.33.0",
    "mysql2": "^3.1.2",
    "prettier": "^2.8.4",
    "shellcheck": "^2.2.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.12",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "typeorm",
    "migrate",
    "sql",
    "migration",
    "migrations",
    "db-migrate",
    "cli",
    "database",
    "db",
    "mysql",
    "mariadb"
  ],
  "author": "2020leon <leon2000.tw@gmail.com>",
  "homepage": "https://github.com/2020leon/typeorm-migrate-sql",
  "repository": "github:2020leon/typeorm-migrate-sql",
  "license": "MIT"
}
